{
  "name": "bolierplate-npm-webpack-backbone",
  "version": "1.0.0",
  "description": "Boilerplate for Web apps with npm, webpack and backbone",
  "main": "",
  "scripts": {
    "prebuild": "rimraf public",
    "build": "webpack -p",
    "webpack-watch": "webpack -w",
    "express-server": "nodemon ./server",
    "dev": "concurrently --kill-others \"npm run webpack-watch\" \"npm run express-server\"",
    "start": "npm run build && node ./server",
    "test": "mocha --require test/helpers/setup.js --recursive",
    "cover": "nyc --reporter=lcov --reporter=text --reporter=html --require babel-register npm test"
  },
  "author": "Gustavo Morales <gustavo.morales@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-register": "^6.14.0",
    "chai": "^3.5.0",
    "concurrently": "^2.2.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.24.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "jsdom": "^9.5.0",
    "jshint": "^2.9.3",
    "jshint-loader": "^0.8.3",
    "mocha": "^3.0.2",
    "node-sass": "^3.8.0",
    "nodemon": "^1.10.2",
    "nyc": "^8.1.0",
    "rimraf": "^2.5.4",
    "sass-lint": "^1.9.1",
    "sass-loader": "^4.0.0",
    "sasslint-loader": "0.0.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.0"
  },
  "dependencies": {
    "backbone": "^1.3.3",
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.7",
    "express": "^4.14.0",
    "handlebars": "^4.0.5",
    "handlebars-loader": "^1.4.0",
    "jquery": "^3.1.0",
    "lodash": "^4.15.0",
    "morgan": "^1.7.0",
    "pug": "^2.0.0-beta6"
  }
}
